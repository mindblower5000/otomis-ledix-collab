define_function RefreshLanguage(DEV dvTP, CHAR language)
{
	SEND_COMMAND dvTP,	"'^TXT-', ITOA(ADDR_START),',0,', 		TEXT_START[language][ADDR_START - ADDR_START_OFFSET]"
	SEND_COMMAND dvTP,	"'^TXT-', ITOA(ADDR_ROOM_NAME),',0,', 	rooms[TPNumberRooms(dvTP)].name"	//TEXT_START[language][ADDR_ROOM_NAME - ADDR_START_OFFSET]"
	SEND_COMMAND dvTP,	"'^TXT-', ITOA(ADDR_ROOM_ASSIT),',0,', 	TEXT_START[language][ADDR_ROOM_ASSIT - ADDR_START_OFFSET]"
