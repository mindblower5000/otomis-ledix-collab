DEFINE_CALL 'sendUnicodeString' (DEV dvTP, INTEGER adCode, CHAR value[100])
{
	local_var widechar txt[100]
	
	txt = _WC(value)
	
	SEND_COMMAND dvTP, "'^UNI-', ITOA(adCode), ',0,', WC_TP_ENCODE(txt)"
}
